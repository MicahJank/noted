{
  "name": "Noted",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Testing out React extensions",
  "background": {
      "scripts": [
          "background.js"
      ],
      "persistent": false
  },
  "browser_action": {
      "default_title": "Noted Chrome Extension"
  },
  "permissions": [
      "http://*/",
      "https://*/",
      "activeTab",
      "storage"
  ]
}
